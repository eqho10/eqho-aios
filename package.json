{
  "name": "@eqho10/eqho-aios",
  "version": "0.1.0",
  "description": "AI-Orchestrated Full-Stack Development Framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "eqho-aios": "dist/bin/eqho-aios.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx bin/eqho-aios.ts",
    "start": "node dist/bin/eqho-aios.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["ai", "cli", "development", "framework", "claude", "anthropic", "agents", "orchestration"],
  "author": "Ekrem Mutlu <eqho10@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eqho10/eqho-aios"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist",
    "agents",
    "templates",
    "README.md",
    "README.tr.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "yaml": "^2.4.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/node": "^22.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.0",
    "vitest": "^1.6.0"
  }
}
